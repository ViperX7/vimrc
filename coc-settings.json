{
    let g:coc_global_extensions = [
        "coc-fzf-preview",
        "coc-highlight".
        "coc-tabnine",
        "coc-yank",
        "coc-template",
        "coc-snippets",
        "coc-jedi",
        "coc-git",
        "coc-floaterm",
        "coc-explorer",
        "coc-actions",
        "coc-svg",
        "coc-xml",
        "coc-yaml",
        "coc-html",
        "coc-css",
        "coc-markdownlint",
        "coc-json",
        "coc-sh",
        "coc-python",
        "coc-go",
        "coc-flutter",
        "coc-clangd"
    ]

    "clangd.semanticHighlighting": true,
    "coc.preferences.formatOnSaveFiletypes": [
        "css",
        "markdown",
        "javascript",
        "graphql",
        "html",
        "yaml",
        "json",
        "python",
        "dart"
    ],
    "explorer.width": 30,
    "explorer.icon.enableNerdfont": true,
    // python config
    "python.linting.enabled": true,
    "python.jediEnabled": true,
    "python.linting.pylintEnabled": true,
    // Highlighter
    "coc.preferences.colorSupport": true,
    // explorer
    "explorer.previewAction.onHover": false,
    "explorer.keyMappings": {
        "<cr>": [
            "expandable?",
            "expand",
            "open"
        ],
        "v": "open:vsplit"
    }
}
